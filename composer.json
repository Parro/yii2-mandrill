{
    "name": "nickcv/yii2-mandrill",
    "description": "Mandrill Api Integration for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2", "extension", "mandrill", "mailer", "email"],
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Nicola Puddu",
            "email": "n.puddu@outlook.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "mandrill/mandrill": "dev-master",
        "mailchimp/mailchimp": "^2.0.6"
    },
    "require-dev": {
        "codeception/codeception": "*",
        "victorjonsson/markdowndocs": "*",
        "yiisoft/yii2-coding-standards": "~2.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "https://bitbucket.org/mauroparolari/mandrill-api-php.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "nickcv\\mandrill\\": "src/"
        }
    }
}
